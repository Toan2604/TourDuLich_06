@using Nhom6_TourDuLich.Sessions
@{ var AdminSession = (UserSession)Session[SessionConnection.ADMIN_SESSION]; }
<!doctype html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <link rel='shortcut icon' href='~/wwwroot/images/imgOverView/logo.png' />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="stylesheet" href="~/wwwroot/Admin/vendor/bootstrap/css/bootstrap.min.css">
    <link href="~/wwwroot/Admin/vendor/fonts/circular-std/style.css" rel="stylesheet">
    <link rel="stylesheet" href="~/wwwroot/Admin/libs/css/style.css">
    <link rel="stylesheet" href="~/wwwroot/Admin/vendor/fonts/fontawesome/css/fontawesome-all.css">
    <link rel="stylesheet" href="~/wwwroot/Admin/vendor/charts/chartist-bundle/chartist.css">
    <link rel="stylesheet" href="~/wwwroot/Admin/vendor/charts/morris-bundle/morris.css">
    <link rel="stylesheet" href="~/wwwroot/Admin/vendor/fonts/material-design-iconic-font/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/wwwroot/Admin/vendor/charts/c3charts/c3.css">
    <link rel="stylesheet" href="~/wwwroot/Admin/vendor/fonts/flag-icon-css/flag-icon.min.css">
    <title>@ViewBag.Title </title>
</head>

<body>
    <!-- ============================================================== -->
    <!-- main wrapper -->
    <!-- ============================================================== -->
    <div class="dashboard-main-wrapper">
        <!-- ============================================================== -->
        <!-- navbar -->
        <!-- ============================================================== -->
        <div class="dashboard-header">
            <nav class="navbar navbar-expand-lg bg-white fixed-top">
                <a class="navbar-brand" href="@Url.Action("Index","Home")">VIETTRAVEL</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse " id="navbarSupportedContent">
                    <ul class="navbar-nav ml-auto navbar-right-top">
                        <li class="nav-item dropdown nav-user">
                            <a class="nav-link nav-user-img" href="#" id="navbarDropdownMenuLink2" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><img src="~/wwwroot/images/user.png" alt="" class="user-avatar-md rounded-circle"></a>
                            <div class="dropdown-menu dropdown-menu-right nav-user-dropdown" aria-labelledby="navbarDropdownMenuLink2">
                                <div class="nav-user-info">
                                    <h5 class="mb-0 text-white nav-user-name">@if (AdminSession != null)
                                    {@AdminSession.FullName} </h5>
                                    <span class="status"></span><span class="ml-2">Đang hoạt động</span>
                                </div>
                                @*<a class="dropdown-item" href="@Url.Action("Index","VietTravel")"><i class="fas fa-power-off mr-2"></i>Về trang chủ</a>*@
                                <a class="dropdown-item" href="@Url.Action("Logout","Users")"><i class="fas fa-power-off mr-2"></i>Đăng xuất</a>
                            </div>
                        </li>
                    </ul>
                </div>
            </nav>
        </div>
        <!-- ============================================================== -->
        <!-- end navbar -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- left sidebar -->
        <!-- ============================================================== -->
        <div class="nav-left-sidebar sidebar-dark">
            <div class="menu-list">
                <nav class="navbar navbar-expand-lg navbar-light">
                    <a class="d-xl-none d-lg-none" href="#">Dashboard</a>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarNav">
                        <ul class="navbar-nav flex-column">
                            <li class="nav-divider">
                                Menu
                            </li>

                            <!-- sửa by AnhLe -->
                            <li class="nav-item ">
                                <a class="nav-link" href="@Url.Action("Index","Categories")" aria-expanded="false" data-target="#submenu-1" aria-controls="submenu-1"><i class="fas fa-table"></i> Quản lý danh mục tour <span class="badge badge-success">6</span></a>

                            </li>
                            <li class="nav-item ">
                                <a class="nav-link" href="@Url.Action("Index","Tours")" aria-expanded="false" data-target="#submenu-1" aria-controls="submenu-1"><i class="fas fa-th"></i> Quản lý tour <span class="badge badge-success">6</span></a>

                            </li>
                            <li class="nav-item ">
                                <a class="nav-link" href="@Url.Action("Index","Users")" aria-expanded="false" data-target="#submenu-1" aria-controls="submenu-1"><i class="fas fa-users"></i> Quản lý tài khoản <span class="badge badge-success">6</span></a>

                            </li>
                            <li class="nav-item ">
                                <a class="nav-link" href="@Url.Action("Index","Orders")" aria-expanded="false" data-target="#submenu-1" aria-controls="submenu-1"><i class="fas fa-shopping-basket"></i> Quản lý đơn hàng <span class="badge badge-success">6</span></a>

                            </li>
                            <li class="nav-item ">
                                <a class="nav-link" href="@Url.Action("Index","News")" aria-expanded="false" data-target="#submenu-1" aria-controls="submenu-1"><i class="fas fa-newspaper"></i> Quản lý tin tức <span class="badge badge-success">6</span></a>
                            </li>
                            <!-- Sửa by AnhLe -->
                        </ul>
                    </div>
                </nav>
            </div>
        </div>
        <!-- ============================================================== -->
        <!-- end left sidebar -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- wrapper  -->
        <!-- ============================================================== -->
        <div class="dashboard-wrapper">
            <div class="dashboard-ecommerce">
                <div class="container-fluid dashboard-content ">
                    @RenderBody()
                </div>
            </div>
            <!-- ============================================================== -->
            <!-- footer -->
            <!-- ============================================================== -->
            <div class="footer">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12">
                            Copyright © Nhóm 6 - CNTT4-K13.
                        </div>
                        <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12">
                            <div class="text-md-right footer-links d-none d-sm-block">
                                <a href="javascript: void(0);">Về chúng tôi</a>
                                <a href="javascript: void(0);">Hỗ trợ</a>
                                <a href="javascript: void(0);">Liên hệ với chúng tôi</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- ============================================================== -->
            <!-- end footer -->
            <!-- ============================================================== -->
        </div>
        <!-- ============================================================== -->
        <!-- end wrapper  -->
        <!-- ============================================================== -->
    </div>
    <!-- ============================================================== -->
    <!-- end main wrapper  -->
    <!-- ============================================================== -->
    <!-- Optional JavaScript -->
    <!-- jquery 3.3.1 -->
    <script src="~/wwwroot/Admin/vendor/jquery/jquery-3.3.1.min.js"></script>
    <!-- bootstap bundle js -->
    <script src="~/wwwroot/Admin/vendor/bootstrap/js/bootstrap.bundle.js"></script>
    <!-- slimscroll js -->
    <script src="~/wwwroot/Admin/vendor/slimscroll/jquery.slimscroll.js"></script>
    <!-- main js -->
    <script src="~/wwwroot/Admin/libs/js/main-js.js"></script>
    <!-- chart chartist js -->
    <script src="~/wwwroot/Admin/vendor/charts/chartist-bundle/chartist.min.js"></script>
    <!-- sparkline js -->
    <script src="~/wwwroot/Admin/vendor/charts/sparkline/jquery.sparkline.js"></script>
    <!-- morris js -->
    <script src="~/wwwroot/Admin/vendor/charts/morris-bundle/raphael.min.js"></script>
    <script src="~/wwwroot/Admin/vendor/charts/morris-bundle/morris.js"></script>
    <!-- chart c3 js -->
    <script src="~/wwwroot/Admin/vendor/charts/c3charts/c3.min.js"></script>
    <script src="~/wwwroot/Admin/vendor/charts/c3charts/d3-5.4.0.min.js"></script>
    <script src="~/wwwroot/Admin/vendor/charts/c3charts/C3chartjs.js"></script>
    <script src="~/wwwroot/Admin/libs/js/dashboard-ecommerce.js"></script>
    @RenderSection("scripts", required: false)
    @RenderSection("Messages", required: false)
</body>

</html>



